      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Ui.Books.Pages"
      mc:Ignorable="d" 
      DataContext="{Binding About, Source={StaticResource Locator}}"
      d:DesignHeight="300" d:DesignWidth="300"
      Title="AboutPage">

    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="31.42" TextWrapping="Wrap" Text="{Binding Path=WindowTitle, Mode=OneWay}" VerticalAlignment="Top" Width="95.983"/>

    </Grid>
</Page>
